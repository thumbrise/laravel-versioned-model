{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "thumbrise/laravel-versioned-model",
  "authors": [
    {
      "name": "Ruslan Kokoev",
      "email": "ruslan.kokoev.1999@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Thumbrise\\LaravelVersionedModel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thumbrise\\LaravelVersionedModel\\Tests\\": "tests/"
    }
  },
  "license": [
    "MIT"
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0",
    "spatie/laravel-package-tools": "^1.9"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.75",
    "larastan/larastan": "*",
    "orchestra/testbench": "^6.0 || ^7.0 || ^8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "description": "Versioned model for Laravel.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "scripts": {
    "fmt": [
      "@php vendor/bin/php-cs-fixer fix"
    ],
    "lint": [
      "@php vendor/bin/php-cs-fixer check",
      "@php vendor/bin/phpstan analyse",
      "@composer audit"
    ],
    "test": [
      "./vendor/bin/phpunit --testsuite Unit,Feature --fail-on-risky --fail-on-warning"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  }
}
